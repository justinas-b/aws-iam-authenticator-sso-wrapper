    "version": "0.2.0",
    "configurations": [
        {
            "name": "Go: Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "-src-namespace", "aws-iam-authenticator-sso-wrapper",
                "-src-configmap", "aws-auth",
                "-dst-namespace", "kube-system",
                "-dst-configmap", "aws-auth",
                "-debug"
            ],
            "envFile": "${workspaceFolder}/.env",
        }
    ]
}